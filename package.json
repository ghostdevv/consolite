{
  "name": "consolite",
  "version": "0.1.0",
  "description": "Light wrapper for console",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    }
  },
  "scripts": {
    "test": "node ./test",
    "build": "npx tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakobrosenberg/consolite.git"
  },
  "keywords": [
    "log",
    "logging",
    "debug",
    "console",
    "print"
  ],
  "author": "jakobrosenberg@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakobrosenberg/consolite/issues"
  },
  "homepage": "https://github.com/jakobrosenberg/consolite#readme",
  "prettier": {
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "all",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "semi": false,
    "useTabs": false,
    "printWidth": 90
  }
}
